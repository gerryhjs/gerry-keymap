<keymap version="1" name="Gerry Keymap" parent="$default">
    <action id="ShowSettings">
        <keyboard-shortcut first-keystroke="ctrl alt s"/>
        <keyboard-shortcut first-keystroke="alt s"/>
    </action>
    <action id="FindInPath">
        <keyboard-shortcut first-keystroke="shift ctrl f"/>
        <keyboard-shortcut first-keystroke="alt f"/>
    </action>
    <action id="ReplaceInPath">
        <keyboard-shortcut first-keystroke="shift ctrl r"/>
        <keyboard-shortcut first-keystroke="alt r"/>
    </action>
    <action id="SelectInProjectView">
        <keyboard-shortcut first-keystroke="alt c" />
    </action>
    <action id="PasteMultiple">
        <keyboard-shortcut first-keystroke="shift ctrl v" />
        <keyboard-shortcut first-keystroke="shift ctrl insert" />
        <keyboard-shortcut first-keystroke="alt v" />
    </action>
    <action id="EditorToggleColumnMode">
        <keyboard-shortcut first-keystroke="shift alt insert"/>
        <keyboard-shortcut first-keystroke="alt b"/>
    </action>
    <action id="plugin.InstallFromDiskAction">
        <keyboard-shortcut first-keystroke="alt n" />
    </action>
    <action id="LocalHistory.ShowHistory">
        <keyboard-shortcut first-keystroke="alt m" />
    </action>
    <action id="RecentFiles">
        <keyboard-shortcut first-keystroke="ctrl e" />
        <keyboard-shortcut first-keystroke="alt l" />
    </action>
    <action id="Register">
        <keyboard-shortcut first-keystroke="alt k" />
    </action>
    <action id="OpenFile">
        <keyboard-shortcut first-keystroke="alt o" />
    </action>
    <action id="WelcomeScreen.Plugins">
        <keyboard-shortcut first-keystroke="alt p"/>
    </action>
    <action id="RestartIde">
        <keyboard-shortcut first-keystroke="escape" second-keystroke="escape"/>
    </action>
</keymap>